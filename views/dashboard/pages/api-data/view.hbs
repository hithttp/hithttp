<!-- Page Wrapper -->
<div id="wrapper">

  <!-- Sidebar -->
  {{>"sidebar"}}
  <!-- End of Sidebar -->


  <!-- Content Wrapper -->
  <div id="content-wrapper" class="d-flex flex-column">

    <!-- Main Content -->
    <div id="content">

      <!-- Topbar -->
      {{>"topbar"}}
      <!-- End of Topbar -->

      <!-- Begin Page Content -->
      <div class="container-fluid">

        <!-- Page Heading -->
        {{!-- <h1 class="h3 mb-4 text-gray-800">Models</h1> --}}
        <!-- DataTales Example -->
        <div class="card shadow mb-4">
          <div class="card-header py-3">
            <h5 class="m-0 font-weight-bold text-primary">Detailed Entry
              <a href="/dashboard/resource/{{resId}}/api-data" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm float-right"><i
                  class="fas fa-arrow-left fa-sm text-white"></i> Back</a>
            </h5>
          </div>
          <div class="card-body">
            <table class="table table-bordered">
              <thead>
                <tr>
                  <th>Key</th>
                  <th>Value</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td>Id</td>
                  <td>{{apiData.id}}</td>
                </tr>
                {{#each apiData.data as |value|}}
                <tr>
                  <td>{{@key}}</td>
                  <td>{{value}}</td>
                </tr>
                {{/each}}
                <tr>
                  <td>CreatedAt</td>
                  <td>{{format-date apiData.createdAt}}</td>
                </tr>
                <tr>
                  <td>Last Updated At</td>
                  <td>{{format-date apiData.updatedAt}}</td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <!-- /.container-fluid -->

    </div>
    <!-- End of Main Content -->
    <!-- Footer -->
    {{>"footer"}}
    <!-- End of Footer -->
  </div>
  <!-- End of Content Wrapper -->

</div>
<!-- End of Page Wrapper -->